{
  "catalogs": [
    {
      "path": "src/locales/{locale}/messages",
      "include": [
        "src"
      ]
    }
  ],
  "extractBabelOptions": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-react"
    ]
  },
  "format": "po",
  "formatOptions": {
    "origins": true,
    "lineNumbers": false
  },
  "orderBy": "origin",
  "locales": [
    "en",
    "es",
    "ta",
    "zh",
    "kn"
  ],
  "sourceLocale": "en",
  "fallbackLocales": {
    "default": "en"
  }
}
